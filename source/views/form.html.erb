<div id="contact" class="container d-flex contact-skills-container">
  <div class="contact w-100 mr-3">
    <h1 class="container title">Contact me!</h1>
    <form action="https://getform.io/f/a305fbe2-6f2a-49b7-ad6c-1c2f2e355787" method="POST" class="form-emanuel">
      <div class="form-group">
        <label for="email">Email</label>
        <input type="email" class="form-control" id="email" placeholder=" email" required=true>
      </div>
      <div class="form-group">
        <label for="name">Name</label>
        <input type="text" class="form-control" id="name" placeholder=" name" required=true>
      </div>
      <div class="form-group">
        <label for="message">Message</label>
        <textarea name="message" id="message" cols="12" rows="8" class="form-control"></textarea>
      </div>
      <div class="form-group">
        <button type="submit" value="submit" class="btn btn-light w-100 botoncito">Send!</button>
      </div>
    </form>
  </div>
  <div class="skills w-100">
    <h1 class="container title">My Skills</h1>
    <div class="container skills-container">
      <% data.icons.each do |elements| %>
        <% elements.icons.each do |icon| %>
          <div class="container icon-skill">
            <%= image_tag icon, class:"icon-skill-img" %>
          </div>
        <% end %>
      <% end %>
    </div>
  </div>
</div>
